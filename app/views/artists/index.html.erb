<h1>All Artists</h1>
<div>
<% @artists.each do |artist| %>
  <p>
    <%= artist.name %>
    -
    <%= link_to("edit", edit_artist_path(artist.id)) %>
    -
    <%= link_to("delete", artist_path(artist.id), method: :delete, data: {confirm: "Are you sure?"})%>
  </p>
<% end %>
</div>
<nav class="bottomnav"><%= link_to("New_Artist", new_artist_path)%></nav>
